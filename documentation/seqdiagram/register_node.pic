.PS
copy "sequence.pic"

#boxwid = 1
movewid = 2.7

# Object definition
object(N,"UA")
object(S,"NS")
step

# Bootstrap - register node IP adress
step

# Registration on Notification Server
active(N)
message(N,S,"GET /register/node?t=NODE_UUID&ip=IP")
active(S)
message(S,S,"check_node(NODE_UUID); check_ip(IP) / delete")
message(S,S,"store(NODE_UUID,IP)")
return_message(S,N,"REGISTERED")
inactive(S)
step
inactive(N)

step
step

active(N)
message(N,S,"GET /register/node?t=NODE_UUID&tel=MSISDN")
active(S)
message(S,S,"check_node(NODE_UUID); check_tel(MSISDN) / delete")
message(S,S,"store(NODE_UUID,MSISDN)")
return_message(S,N,"REGISTERED")
inactive(S)
step
inactive(N)

# End diagram
step
complete(N)
complete(S)

