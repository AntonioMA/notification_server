	<subDataset name="New Dataset 2" uuid="7cb134cf-3f50-46b9-9a1e-50429e047aac">
		<queryString>
			<![CDATA[SELECT (DATE_SUB(CURDATE(), INTERVAL 1 DAY))as RDay, temp2.terminales - temp1.terminales AS TermDiff, temp2.activas - temp1.activas AS ActDiff
FROM(

( SELECT (FROM_UNIXTIME(fecha-MOD(fecha,3600))) as Date2,
terminales, activas
FROM kpisdb.mongo_combinados
WHERE (STR_TO_DATE( FROM_UNIXTIME(fecha),'%Y-%m-%d %H:%i:%s')
   BETWEEN DATE_FORMAT(DATE_SUB(CURDATE(), INTERVAL 1 DAY),'%Y-%m-%d %H:%i:%s')
   AND DATE_FORMAT(DATE_SUB(CURDATE(), INTERVAL 1 SECOND),'%Y-%m-%d %H:%i:%s'))
GROUP BY FROM_UNIXTIME(fecha-MOD(fecha,3600))
ORDER BY (FROM_UNIXTIME(fecha-MOD(fecha,3600))) DESC
LIMIT 1) AS temp2,
( SELECT (FROM_UNIXTIME(fecha-MOD(fecha,3600))) as Date1,
terminales, activas
FROM kpisdb.mongo_combinados
WHERE (STR_TO_DATE( FROM_UNIXTIME(fecha),'%Y-%m-%d %H:%i:%s')
   BETWEEN DATE_FORMAT(DATE_SUB(CURDATE(), INTERVAL 1 DAY),'%Y-%m-%d %H:%i:%s')
   AND DATE_FORMAT(DATE_SUB(CURDATE(), INTERVAL 1 SECOND),'%Y-%m-%d %H:%i:%s'))
GROUP BY FROM_UNIXTIME(fecha-MOD(fecha,3600))
ORDER BY (FROM_UNIXTIME(fecha-MOD(fecha,3600))) ASC
LIMIT 1) AS temp1
)]]>
		</queryString>
		<field name="RDay" class="java.sql.Date"/>
		<field name="TermDiff" class="java.lang.Long"/>
		<field name="ActDiff" class="java.lang.Long"/>
	</subDataset>
	<queryString>
